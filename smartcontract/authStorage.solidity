pragma solidity ^0.4.24;

contract authStorage{
    
    struct Account{
        bytes32 id;
        address wallet;
    }
    
    mapping (bytes32 => Account) accounts;

    function signUp(bytes32 _id, address _wallet) private {
        
        Account storage account = accounts[_id];
        
        account.id = _id;
        account.wallet = _wallet;
        
    }
 
}